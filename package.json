    "concurrently": "^3.5.1",
    "config": "^3.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.16.2",
    "express-ipfilter": "^1.0.1",
    "moment": "^2.24.0",
    "mongoose": "^5.9.18",
    "mysql": "^2.17.1",
    "nodemailer": "^6.3.1",
    "socket.io": "^2.3.0"
  }
}
